	"page": {
		"$type": "Page",
		"id": 1,
		"layout": [
			{
				"$type": "Hero",
				"richText": "<h2><strong>Janilla</strong> is an advanced <br /> yet plain vanilla Java module <br /> pursuing <a href=\"https://en.wikipedia.org/wiki/Overengineering\" target=\"_blank\">Overengineering</a>-free <br /> Web Development</h2>"
			},
			{
				"$type": "Features",
				"title": "Packages",
				"items": [
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/cms\" target=\"_blank\">CMS</a>",
						"text": "Models content as documents and files. Provides API endpoints to read and write content. Generates the Admin Panel. Supports versions and drafts."
					},
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/frontend\" target=\"_blank\">Frontend</a>",
						"text": "Simplifies web component development by replacing manual DOM manipulation with declarative HTML template interpolation."
					},
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/http\" target=\"_blank\">HTTP</a>",
						"text": "Provides HTTP/2 server and client."
					},
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/io\" target=\"_blank\">IO</a>",
						"text": "Gives the possibility to make Java NIO channels transactional."
					},
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/ioc\" target=\"_blank\">IoC</a>",
						"text": "Provides a factory with Dependency Injection capability."
					},
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/java\" target=\"_blank\">Java</a>",
						"text": "Handles advanced type conversion. Provides other utilities such as scanning Java modules."
					},
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/json\" target=\"_blank\">JSON</a>",
						"text": "Formats Java objects as JSON strings. Extracts tokens from JSON input. Also provides utilities for JWT."
					},
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/net\" target=\"_blank\">Net</a>",
						"text": "Extends Java NIO to support SSL transfers. Provides a base secure server. Also contains URI's mutable query."
					},
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/persistence\" target=\"_blank\">Persistence</a>",
						"text": "Models data as entities. Defines annotations for stores and indexes. Provides a base for CRUD operations."
					},
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/reflect\" target=\"_blank\">Reflect</a>",
						"text": "Scans Java types for object properties. Allows dynamic inspection and manipulation of arbitrary objects."
					},
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/smtp\" target=\"_blank\">SMTP</a>",
						"text": "Sends emails."
					},
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/sqlite\" target=\"_blank\">SQLite</a>",
						"text": "Reads and writes data into tables and indexes stored as B-Trees in a single file, using SQLite format."
					},
					{
						"$type": "Feature",
						"title": "<a href=\"https://github.com/diego-schivo/janilla/tree/main/source/com/janilla/web\" target=\"_blank\">Web</a>",
						"text": "Serves static files or dynamically invokes Java methods in HTTP exchanges. Handles request data binding and result rendering in response."
					}
				]
			},
