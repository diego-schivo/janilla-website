	"page": {
		"$type": "Page",
		"id": 1,
		"layout": [
			{
				"$type": "Hero",
				"richText": "<h2>Janilla provides a <strong>full web solution stack</strong><br />packed into a single lightweight library</h2>"
			},
			{
				"$type": "Features",
				"items": [
					{
						"$type": "Feature",
						"title": "Vanilla everywhere",
						"text": "Just plain Java for the backend and plain JavaScript, HTML and CSS for the frontend, all in their most modern form: there are no additional entry barriers."
					},
					{
						"$type": "Feature",
						"title": "Truly open-source",
						"text": "Reasonably few and short source files, simply organized to facilitate quick and comprehensive understanding: the learning curve is pretty approachable."
					},
					{
						"$type": "Feature",
						"title": "Problem solving driven",
						"text": "Getting your hands dirty with computing is the key to produce quality software in an efficient way: hence our library comes with a rich set of sample applications."
					},
					{
						"$type": "Feature",
						"title": "Backed by Computer science",
						"text": "Core library components have been implemented leveraging efficient data structures, and we are committed to further advance the state-of-the-art."
					},
					{
						"$type": "Feature",
						"title": "Developer friendly",
						"text": "Few concepts to learn a dependency-free and compact architecture based on mature and well-established technologies, to offer a smooth productivity experience."
					},
					{
						"$type": "Feature",
						"title": "Everything under control",
						"text": "Only one thin library separates the application from the server and the browser, and you have the possibility to access, debug and improve any component in any layer."
					}
				]
			},
			{
				"$type": "ExampleApps",
				"title": "Example apps",
				"examples": [
					1,
					2,
					3,
					4,
					5,
					6,
					7
				]
			}
		],
		"status": "PUBLISHED"
	},
